{
    "2": {
        "50_0": {
            "lines": [
                "Hahahahaha !",
                "Quelque chose de vraiment intéressant va arriver !"
            ]
        },
        "180_0": {
            "lines": [
                "Allez les fées, ",
                "plus vite, plus vite !"
            ]
        },
        "330_0": {
            "lines": [
                "Place à la folie !",
                "Bienvenue dans le monde démentiel !"
            ]
        }
