		etOffset(2, cos(Angle)*60f,sin(Angle)*60f); 
		etAngle(2, Angle+sin(sinus), 0f);
		etOn(2);
		Angle += rad(90);
		}
		Angle += rad(2.15);
		sinus += rad(12);
		wait(1);
	}
}

