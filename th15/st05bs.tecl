void Spirale()
{
	float Angle = rad(90);
	float sinus = 0f;
	etNew(0);
	etAim(0, 3);
	etCount(0, 1, 1);
	etSprite(0, 15, 14);
	etSpeed(0, 3f:2.5f:2f:2f, 3f:2.5f:2f:2f);
	etEx(0, 0, EX_ANIM, 0, NEG, NEGF, NEGF);
	times(300)
	{
		times(4)
		{
		etOffset(0, cos(Angle)*60f,sin(Angle)*60f); 
		etAngle(0, Angle+sin(sinus), 0f);
		etOn(0);
		Angle += rad(90);
		}
		Angle += rad(2.15);
		sinus += rad(12);
		wait(1);
	}
}

void Spirale2()
{
