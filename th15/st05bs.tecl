//pattern a garder
void Horizon()
{
	float anglepos = RANDRAD;
	float differ = 0f;
	float shrink = 100f;
    etNew(0);
	etAim(0, 3);
	etCount(0, 1, 1);
	etSprite(0, 11, 13);
	etSpeed(0, 4f, 4f);
	times(100)
	{
		anglepos += rad(7);
		times(2)
		{
			differ += rad(180);
			etOffset(0, cos(anglepos+differ)*shrink*0.5f, sin(anglepos+differ)*shrink);
			etAngle(0, anglepos+differ, 0f);
			etOn(0);
		}
		shrink += -1f;
		wait(1);
	}
}

