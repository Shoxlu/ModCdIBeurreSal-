void UltimateGirl_lasers(int A, int etID, float isNeg){
	int B = 1;
	float startAngle = (rad(45)*isNeg):0f:0f:rad(22.5);
	times(A){
		laserStOn(etID, B);
		laserStAngle(B, startAngle+rad(360)/_f(A) * _f(B));
		laserOffset(B, ABS_X+cos(startAngle+rad(360)/_f(A) * _f(B))*50f, ABS_Y+sin(startAngle+rad(360)/_f(A) * _f(B))*50f);
		B += 1;
	}
	float Angle = 0f;
	float laserAngle = 0f;
	wait(110);
	playSound(18);
	times(100){
		B = 1;
		times(A){
			laserAngle = startAngle+Angle+rad(360)/_f(A) * _f(B);
			laserStAngle(B, laserAngle);
			laserOffset(B, ABS_X+cos(laserAngle)*50f, ABS_Y+sin(laserAngle)*50f);
			B += 1;
		}
		Angle += rad(0.2)*isNeg;
		wait(1);
	}
	wait(60);
}

