void GirlB_at()
{
	float angle = 0f;
	int count = 6:8:10:15;
	float inc = rad(360)/_f(count);
	etNew(0);
	etAim(0, 1);
	etSpeed(0, 2f, 2.5f:3f:3f);
	etCount(0, 1, 1:2:3:3);
	etSprite(0, 12, 15);//to determine
	wait(30);
	etEx(0, 0, EX_ANIM, 1, NEG, NEGF, NEGF);
	while(1){
		angle = ANGLE_PLAYER;
		times(count){
			etAngle(0, angle, 0f:0f:rad(5):rad(5));
			etOn(0);
			angle += inc;
			wait(2:2:1:1);
		}
		wait(60:60:40:30);
	}
}


