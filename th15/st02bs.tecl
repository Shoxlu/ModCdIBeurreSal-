	{
		movePos(cos(GF1+F0) * _f(LIFE)+BOSS_X, sin(GF1+F0)* _f(LIFE)+BOSS_Y);
		wait(1);
	}
	times(10)
	{
		A = A - (170f/10f);
		setHitbox(A, A);
		wait(1);
	}
	flagSet(3);
}

