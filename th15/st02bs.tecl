void Boss1_at()
{
	F0 = rad(90);
	int B = 3:4:5:5;
	I0 = 1;
	I1 = 1;
	I2 = 2;
	times(B)
	{
		enmCreateA("Spawn", cos(F0)*70f, sin(F0)*70f, 70, 10, 1);
		F0 = F0 + rad(360)/_f(B);
		I1 += 1;
		if(I1 == 7)
		{
			I1 = 1;
		}
	}
	F0 = rad(270);
	I0 = -1;
	I2 = 15;
	times(B)
	{
		enmCreateA("Spawn", cos(F0)*100f, sin(F0)*100f, 100, 10, 1);
		F0 = F0 + rad(360)/_f(B);
		I1 += 1;
		if(I1 == 7)
		{
			I1 = 1;
		}
	}
}
