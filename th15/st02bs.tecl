void BossCard1_at(float C)
{
	float A = RANDRAD;
	int B = 0;
	etNew(1);
	etAim(1, 3);
	etSprite(1, 23, 1 + RAND % 6);
	etCount(1, 4, 1);
	etAngle(1, A, 0f);
	etSpeed(1, 1f, 1f);
	etExSet(1, 0, 0, EX_ANIM, 1, NEG, NEGF, NEGF);
	etEx(1, 0, EX_ACCEL, 60, NEG, -1f/60f, NEGF);
	etEx(1, 0, EX_WAIT, 60, NEG, NEGF, NEGF);
	etEx(1, 0, EX_ACCEL, 120, 0, 2f/120f, NEGF);
	B = 2;
	etSprite(1, 23, B);
	times(4:6:7:11){
		etOn(1);
		wait(10:10:9:8);
		A = A + (rad(13):rad(13):rad(15):rad(13)) * C;
		etAngle(1, A, 0f);
		etSprite(1, 23, B);
		etOn(1);
		wait(10:10:9:8);
		A = A + (rad(13):rad(13):rad(15):rad(13)) * C;
		etAngle(1, A, 0f);
		etSprite(1, 23, B - 1);
	}
}

void MasterSpark()
{

	int waitTime = 1;
	float A = 0;
	int loop = 300:300:320:350;
	flagSet(3);
	deathAnm(0, -1);
	A = ANGLE_PLAYER+rad(40);
	GF1 = A;
	anmSelect(4);
    anmSetSprite(0, 0);//sprite MasterSpark
	anmRotate(0, A+F0);
	BossTrace() async;
    MasterSparkHitbox() async;
    wait(60);
    setScreenShake(loop*waitTime, 1, 1);
	playSound(49);
    times(loop)//rotation MasterSpark
    {
        anmRotate(0, A+F0);
        wait(waitTime);
		A = A + (rad(1)/(_f(loop)/40f))*_f(I0);
        GF1 = A;
    }
	GI0 = 1;
    anmSwitch(0, 1);//disparition MasterSpark
    wait(6000);
}
