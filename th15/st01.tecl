void Bowap()
{
	float A = rad(-10);
	float B = rad(0);
	etNew(9);
	etAim(9, 3);
	etAngle(9, A, 0f);
	etCount(9, 8, 1);
	etSpeed(9, 2f, 2f);
	etSprite(9, 8, 8);
	while(1)
	{
		etOn(9);
